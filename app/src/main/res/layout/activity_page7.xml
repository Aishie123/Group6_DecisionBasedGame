<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View.Page7">

  <ImageView
        android:id="@+id/temporaryBG"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:adjustViewBounds="true"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/bg_bedroom" />

    <ImageView
        android:id="@+id/darkShade7"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0.8"
        android:src="@drawable/darkshade"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/txt7Dialogue"
        android:layout_width="398dp"
        android:layout_height="170dp"
        android:fontFamily="@font/nicoclean_regular"
        android:gravity="center"
        android:text="Dialogue"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/btn7Choice1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/temporaryBG"
        app:layout_constraintVertical_chainStyle="spread" />

    <ImageButton
        android:id="@+id/btn7Choice1"
        android:layout_width="116dp"
        android:layout_height="60dp"
        android:padding="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/temporaryBG"
        app:layout_constraintEnd_toStartOf="@+id/btn7Choice2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/temporaryBG"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt7Dialogue"
        app:srcCompat="@drawable/btn_unpressed"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/btn7Choice2"
        android:layout_width="116dp"
        android:layout_height="60dp"
        android:padding="0dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toStartOf="@+id/btn7Choice3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn7Choice1"
        app:layout_constraintTop_toTopOf="@+id/btn7Choice1"
        app:srcCompat="@drawable/btn_unpressed"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/btn7Choice3"
        android:layout_width="116dp"
        android:layout_height="60dp"
        android:padding="0dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toStartOf="@+id/btn7Choice4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn7Choice2"
        app:layout_constraintTop_toTopOf="@+id/btn7Choice2"
        app:srcCompat="@drawable/btn_unpressed"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/btn7Choice4"
        android:layout_width="116dp"
        android:layout_height="60dp"
        android:padding="0dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn7Choice3"
        app:layout_constraintTop_toTopOf="@+id/btn7Choice3"
        app:srcCompat="@drawable/btn_unpressed"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/txt7Choice1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nicobold_regular"
        android:text="Choice 1"
        android:textColor="#5e6978"
        app:layout_constraintBottom_toBottomOf="@+id/btn7Choice1"
        app:layout_constraintEnd_toEndOf="@+id/btn7Choice1"
        app:layout_constraintStart_toStartOf="@+id/btn7Choice1"
        app:layout_constraintTop_toTopOf="@+id/btn7Choice1"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/txt7Choice2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nicobold_regular"
        android:text="Choice 2"
        android:textColor="#5e6978"
        app:layout_constraintBottom_toBottomOf="@+id/btn7Choice2"
        app:layout_constraintEnd_toEndOf="@+id/btn7Choice2"
        app:layout_constraintStart_toStartOf="@+id/btn7Choice2"
        app:layout_constraintTop_toTopOf="@+id/btn7Choice2"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/txt7Choice3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nicobold_regular"
        android:text="Choice 3"
        android:textColor="#5e6978"
        app:layout_constraintBottom_toBottomOf="@+id/btn7Choice3"
        app:layout_constraintEnd_toEndOf="@+id/btn7Choice3"
        app:layout_constraintStart_toStartOf="@+id/btn7Choice3"
        app:layout_constraintTop_toTopOf="@+id/btn7Choice3"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/txt7Choice4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nicobold_regular"
        android:text="Choice 4"
        android:textColor="#5e6978"
        app:layout_constraintBottom_toBottomOf="@+id/btn7Choice4"
        app:layout_constraintEnd_toEndOf="@+id/btn7Choice4"
        app:layout_constraintStart_toStartOf="@+id/btn7Choice4"
        app:layout_constraintTop_toTopOf="@+id/btn7Choice4"
        tools:ignore="HardcodedText" />

    <VideoView
      android:id="@+id/death7"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:visibility="gone"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <ImageButton
      android:id="@+id/btn7Restart"
      android:layout_width="116dp"
      android:layout_height="60dp"
      android:layout_marginBottom="60dp"
      android:padding="0dp"
      android:scaleType="fitXY"
      android:visibility="gone"
      app:layout_constraintBottom_toBottomOf="@+id/temporaryBG"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:srcCompat="@drawable/btn_unpressed"
      tools:ignore="SpeakableTextPresentCheck" />

  <TextView
      android:id="@+id/txt7Restart"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/nicobold_regular"
      android:text="Restart"
      android:visibility="gone"
      android:textColor="#5e6978"
      app:layout_constraintBottom_toBottomOf="@+id/btn7Restart"
      app:layout_constraintEnd_toEndOf="@+id/btn7Restart"
      app:layout_constraintStart_toStartOf="@+id/btn7Restart"
      app:layout_constraintTop_toTopOf="@+id/btn7Restart"
      tools:ignore="HardcodedText" />



</androidx.constraintlayout.widget.ConstraintLayout>
